   <div class="modal">
      <div class="modal-container">
         <div class="modal-container__close"><span></span></div>
         <!-- /.modal-container__close -->
         <h2 class="modal-container__title">Свяжитесь с нами</h2>
         <!-- /.modal-container__title -->
         <form action="POST" class="modal-container__form">
            <div class="form-group">
               <label for="name">Имя</label>
               <input type="text" id="name" class="input input-primary" placeholder="Введите имя *" required>
            </div>
            <div class="form-group">
               <label for="phone">Телефон</label>
               <input type="text" id="phone" class="input input-primary" placeholder="Введите телефон *" required>
            </div>
            <div class="form-group">
               <label for="email">Почта</label>
               <input type="email" id="email" class="input input-primary" placeholder="Введите почту *" required>
            </div>
            <div class="form-group">
               <label for="message">Сообщение</label>
               <textarea name="message" id="message" class="textarea textarea-primary" placeholder="Введите дополнительную информацию"></textarea>
            </div>
            <div class="form-policy">
               <input type="checkbox" id="policy" required>
               <label for="policy">Даю согласие на обработку личных данных</label>
            </div>
            <div class="modal-container__btn">
               <input type="submit" value="Отправить" class="btn btn-primary">
            </div>
         </form>
         <!-- /.modal-container__form -->
      </div>
      <!-- /.modal-container -->
   </div>
   <!-- /.modal -->

